From 1eed2c5af334077a27d5007b006db345de9c4d0f Mon Sep 17 00:00:00 2001
From: Andrew Bayer <andrew.bayer@gmail.com>
Date: Tue, 8 Mar 2011 12:50:50 -0800
Subject: [PATCH 0907/1179] CLOUDERA-BUILD. Changing releases repo to point to staging area.

---
 cloudera-pom.xml                 |    2 +-
 cloudera/maven-packaging/pom.xml |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/cloudera-pom.xml b/cloudera-pom.xml
index 6a40930..80ec196 100644
--- a/cloudera-pom.xml
+++ b/cloudera-pom.xml
@@ -71,7 +71,7 @@ NOTE - This POM is used for Cloudera's build and packaging process.
       <repositories>
         <repository>
           <id>cdh.releases.repo</id>
-          <url>https://repository.cloudera.com/content/repositories/releases</url>
+          <url>https://repository.cloudera.com/content/groups/cdh-releases-rcs</url>
           <name>CDH Releases Repository</name>
           <snapshots>
             <enabled>false</enabled>
diff --git a/cloudera/maven-packaging/pom.xml b/cloudera/maven-packaging/pom.xml
index 5fac49c..0afa6b9 100644
--- a/cloudera/maven-packaging/pom.xml
+++ b/cloudera/maven-packaging/pom.xml
@@ -48,7 +48,7 @@
   <repositories>
     <repository>
       <id>cdh.releases.repo</id>
-      <url>https://repository.cloudera.com/content/repositories/releases</url>
+      <url>https://repository.cloudera.com/content/groups/cdh-releases-rcs</url>
       <name>CDH Releases Repository</name>
       <snapshots>
         <enabled>false</enabled>
