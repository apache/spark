   +- LocalRelation [col1#x, col2#x]


-- !query
SELECT col1 FROM VALUES(1,2) GROUP BY col1 HAVING MAX(col2) == (SELECT 1 WHERE MAX(col2) = 1)
-- !query analysis
Project [col1#x]
+- Filter (max(col2)#x = scalar-subquery#x [max(col2)#x])
   :  +- Project [1 AS 1#x]
   :     +- Filter (outer(max(col2)#x) = 1)
   :        +- OneRowRelation
   +- Aggregate [col1#x], [col1#x, max(col2#x) AS max(col2)#x]
      +- LocalRelation [col1#x, col2#x]


