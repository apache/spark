{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1,
        "MemberExpression": "off"
        }
    ],
    "no-unused-vars": ["error", {"argsIgnorePattern": "^_ignored_.*"}]
  },
  "ignorePatterns": [
    "*.min.js",
    "sorttable.js",
    "jquery.mustache.js",
    "dataTables.rowsGroup.js"
  ],
  "parserOptions": {
    "sourceType": "module"
  }
}
